{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "max-len": 0,
        "no-plusplus":"off",
        "global-require":"off",
        "no-underscore-dangle":"off",
        "no-param-reassign":"off",
        "no-console":"off"
    }
}